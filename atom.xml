---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>blog.disktree.net</title>
	<link href="http://blog.disktree.net/atom.xml" rel="self" />
	<link href="http://blog.disktree.net/" />
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://blog.disktree.net/</id>
	<author>
		<name>tong</name>
		<email>info@disktree.net</email>
	</author>
	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://blog.disktree.net{{ post.url }}" />
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://blog.disktree.net{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>